}if(OAS_version>=11){document.write('<SCRIPT LANGUAGE=JavaScript1.1 SRC="http://sifomedia.idg.se/3/'+OAS_sitepage+"/1"+OAS_RNS+"@"+OAS_listpos+"?"+OAS_query+'"></SCRIPT>');
}document.write("");

