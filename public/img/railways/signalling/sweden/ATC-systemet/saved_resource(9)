var name=".skyscraperWrapper";
var menuYloc=null;
jQuery(document).ready(function(){menuYloc=parseInt(jQuery(name).css("top").substring(0,jQuery(name).css("top").indexOf("px")));
if(/AppleWebKit/i.test(navigator.userAgent)){jQuery(name).css("position","fixed").css("top",menuYloc);
return;
}jQuery(window).scroll(function(){var winHeight=jQuery(window).height();
var adHeight=jQuery(name).height();
var docHeight=jQuery(document).height();
var scrollPos=jQuery(document).scrollTop();
var leftOnAd=((adHeight-winHeight)+10);
var moveUp=0;
if(adHeight>=winHeight){leftover=scrollPos-(docHeight-winHeight);
negativeMovement=((scrollPos+winHeight)-(docHeight-leftOnAd));
if(negativeMovement>0){moveUp=negativeMovement;
}}var offset=((menuYloc+scrollPos)-moveUp)+"px";
jQuery(name).animate({top:offset},{duration:500,queue:false});
});
});

